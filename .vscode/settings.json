{
    "typescript.reportStyleChecksAsWarnings": false,
    "editor.tabSize": 2,
    "files.associations": {
        ".nsupload.json": "jsonc"
    },
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit"
    },
    "todohighlight.exclude": [
        "**/node_modules/**",
        "**/.vscode/**"
    ],
    "todohighlight.include": [
      "**/src/**.*"
    ],
    "todohighlight.isEnable": true
}